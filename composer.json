{
  "name": "wednesdev/mail",
  "description": "Simplify and enhance your email sending capabilities in Laravel 11 projects with this highly extendable and efficient email package.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Wednesdev\\Mail\\": "src/",
      "Wednesdev\\Mail\\Src\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "authors": [
    {
      "name": "omerozoglu",
      "email": "omerozoglu.1@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wednesdev\\Mail\\Src\\Providers\\MailServiceProvider"
      ],
      "aliases": {
        "EmailType": "Wednesdev\\Mail\\Src\\Enums\\EmailType"
      }
    }
  }
}
